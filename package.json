{
    "name": "@ali/htmlonline",
    "version": "2.0.7",
    "description": "html file transfrom",
    "main": "./build/index.js",
    "watch": {
        "test": {
            "patterns": ["src", "test"],
            "extensions": "js,html",
            "quiet": true
        }
    },
    "scripts": {
        "build": "babel src/ --out-dir build/",
        "test": "npm run build && node build/index.js --src=./test/index.html --outDir=./testbuild/ --replaceAttr=taobao --appname=app-test",
        "watch": "npm-watch",
        "publish-npm": "cp package_npm.json package.json && npm run build && npm publish",
        "publish-tnpm": "cp package_tnpm.json package.json && npm run build && tnpm publish"
    },
    "publishConfig": {
        "registry": "http://registry.npm.alibaba-inc.com"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:qingying/htmlonline.git"
    },
    "author": "qingying",
    "license": "MIT",
    "bin": "./build/index.js",
    "dependencies": {
        "yargs": "~5.0.0",
        "cheerio": "0.22.0",
        "path": "~0.12.7",
        "request": "~2.74.0",
        "babel-polyfill": "~6.13.0",
        "colour": "~0.7.1"
    },
    "devDependencies": {
        "webpack": "~1.13.2",
        "babel-cli": "^6.0.0",
        "babel-core": "^6.6.x",
        "babel-preset-es2015": "*",
        "babel-preset-stage-0": "*",
        "babel-preset-stage-1": "*",
        "babel-preset-stage-2": "*",
        "babel-preset-stage-3": "*",
        "npm-watch": "*"
    }
}
